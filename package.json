{
  "name": "nextjs-url-shortener",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "build:1-migrate": "prisma migrate deploy",
    "build:2-build": "next build",
    "build": "run-s build:*",
    "db-up": "docker-compose up -d",
    "db-seed": "prisma db seed",
    "db-migrate-dev": "yarn prisma migrate dev",
    "db-nuke": "docker-compose down --volumes --remove-orphans",
    "dev": "next dev",
    "dx": "run-s db-up db-migrate-dev db-seed dev",
    "start": "next start",
    "lint": "eslint src",
    "lint-fix": "yarn lint --fix",
    "test": "jest",
    "test-dev": "start-server-and-test dev 3000 test:e2e",
    "test-start": "start-server-and-test start 3000 test:e2e",
    "test:e2e": "playwright test",
    "ts-node": "ts-node --compiler-options \"{\\\"module\\\":\\\"commonjs\\\"}\"",
    "postinstall": "prisma generate",
    "prepare": "husky install"
  },
  "prisma": {
    "seed": "yarn ts-node prisma/seed.ts"
  },
  "dependencies": {
    "@chakra-ui/icons": "^1.1.7",
    "@chakra-ui/react": "^1.8.8",
    "@emotion/react": "^11",
    "@emotion/styled": "^11",
    "@prisma/client": "^3.0.1",
    "@trpc/client": "^9.22.0",
    "@trpc/next": "^9.22.0",
    "@trpc/react": "^9.22.0",
    "@trpc/server": "^9.22.0",
    "framer-motion": "^6",
    "hashids": "^2.2.10",
    "lodash": "^4.17.21",
    "next": "12.1.4",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "react-hook-form": "^7.29.0",
    "react-query": "^3.27.0",
    "start-server-and-test": "^1.12.0",
    "zod": "^3.0.0"
  },
  "devDependencies": {
    "@playwright/test": "^1.19.2",
    "@types/jest": "^27.0.1",
    "@types/lodash": "^4.14.181",
    "@types/node": "^17.0.10",
    "@types/react": "^17.0.20",
    "@typescript-eslint/eslint-plugin": "^5.19.0",
    "@typescript-eslint/parser": "^5.14.0",
    "dotenv-cli": "^5.1.0",
    "eslint": "^8.13.0",
    "eslint-config-next": "^12.0.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-react": "^7.25.1",
    "husky": ">=6",
    "jest": "^27.1.0",
    "lint-staged": ">=10",
    "npm-run-all": "^4.1.5",
    "playwright": "^1.19.1",
    "prettier": "^2.5.1",
    "prisma": "^3.7.0",
    "ts-jest": "^27.0.5",
    "ts-node": "^10.3.0",
    "typescript": "^4.6.3"
  },
  "publishConfig": {
    "access": "restricted"
  }
}
